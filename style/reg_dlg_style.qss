
QLabel[tipType="default"]{
    color: black;
    background: transparent;
}

QLabel[tipType="error"]{
    color: white;
    background: #e74c3c;
    border-radius: 4px;
    padding: 2px 6px;
}

QLabel[tipType="success"]{
    color: white;
    background: #27ae60;
    border-radius: 4px;
    padding: 2px 6px;
}

QLabel[tipType="warning"]{
    color: black;
    background: #f1c40f;
    border-radius: 4px;
    padding: 2px 6px;
}
